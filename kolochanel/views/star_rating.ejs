<% if(starhandle) { %>
  <div class="d-flex justify-content-evenly flex-row mb-1">
  <input class="form-control" name="star_vvalue" id="star_vvalue_<%= starhandle %>" value="0.0" type="text" hidden>

  <% for(var i=0; i < 10; i+=2) { %>
    <% var handle = "star_" + i + "_" + starhandle %>
    <% if(rating*2 > (i+1)) { %>
      <img src="/images/icons/star-fill.svg" class="p-1 img-responsive" id="<%=handle%>" onclick="clickon_star('<%=handle%>')" style="width: 20%; height: 20%">
    <% } else if(rating*2 < (1+i)) { %>
      <img src="/images/icons/star.svg" class="p-1 img-responsive" id="<%=handle%>" onclick="clickon_star('<%=handle%>')" style="width: 20%; height: 20%">
    <% } else { %>
      <img src="/images/icons/star-half.svg" class="p-1 img-responsive" id="<%=handle%>" onclick="clickon_star('<%=handle%>')" style="width: 20%; height: 20%">
    <% } %>
  <% } %>
  </div>
<% } else { %>
  <div class="d-flex justify-content-evenly flex-row mb-1">
    <% for(var i=0; i < 10; i+=2) { %>
      <% if(rating*2 > (i+1)) { %>
        <img src="/images/icons/star-fill.svg" class="p-1 img-responsive" style="width: 20%; height: 20%">
      <% } else if(rating*2 < (1+i)) { %>
        <img src="/images/icons/star.svg" class="p-1 img-responsive" style="width: 20%; height: 20%">
      <% } else { %>
        <img src="/images/icons/star-half.svg" class="p-1 img-responsive" style="width: 20%; height: 20%">
      <% } %>
    <% } %>
    </div>  
<% } %>